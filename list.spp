
$ Append 2 lists together
$ list1, list2 : two lists
maki list_append list1 list2 :
	if list_empty list1
		nori list2
	;;

	list_cons (list_head list1) (append (list_tail list1) list2)
;;

list_append {2 3 4} {5 6 7}


$ Reverse a list
$ list: list to reverse
maki reverse list : 
	if (list_len list) <= 1
		nori list
	;;

	list_append (reverse (list_tail list)) (list_head list)
;;

reverse {3 4 1 3 2 5 4 "ER" "Z" 43}