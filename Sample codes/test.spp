maki condition-test : 
	maki a = 3
	if a < 4
		print "Yes"
	else if a == 4
		print "Equal"
	else
		print "Nope."
	;;

	menu a
		1 -> print "1"
		2 -> print "2"
		_ -> print "else"
	;;

;;

maki loop-test-function : 
	roll i <= 5
		print "Test"
		i++
		if i == 3
			break
		;;
	;;

	for int i = 0 to 5
		print "Test2"
		++i
	;;
;;

maki b = "testing" . " some " . "features"
maki test-embedded-closure func : 
	nori func b
;;

test-embedded-closure (soy a : print a)
